<div class="under-nav container">
    <h2>Bibiloteca</h2>
    <h4>Para bajar el archivo haz click en el nombre del archivo!</h4>
    <hr>
    <% if check_user(current_user) == Admin %>
        <%= render "layouts/errors" %>
        <p class="create-new-folder-text">Crear una nueva carpeta</p>
        <div class="create-new-folder">
            <%= render "folders/new" %>
        </div>
    <% end %>
    <br>
    <div class="all-folders">
        <%= render "folders/all" %>
    </div>  
</div>