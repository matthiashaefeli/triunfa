wb = xlsx_package.workbook
wb.add_worksheet(name: "Instructores") do |sheet|
  sheet.add_row ["Nombre", "Apellido", "Email", "Calle", "C.P.", "Ciudad", "Estado", "Tel.", "logins"]
  @teachers.each do |t|
  sheet.add_row [t.user.name, t.user.lastname, 
                t.user.email, t.user.street, 
                t.user.cp, t.user.state, 
                t.user.tel, t.user.login_counts]
  end
end