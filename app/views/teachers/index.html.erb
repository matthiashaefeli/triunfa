<div class="under-nav teacher-index">
  <div class="teacher-inner">
    <div class="teacher-admin-show">
      <%= render "layouts/errors" %>
      <% if @searched_user == Admin %>
        <p class="teacher-show-text"><i class="fas fa-angle-double-down"></i> <%= @spanish ? "Clic aquí para ver los Instructores" : "Click here to see all teachers" %></p>
        <div class="teacher-show-all">
          <%= render "teachers_all" %>
        </div>
        <p class="admin-show-text"><i class="fas fa-angle-double-down"></i> <%= @spanish ? "Clic aquí para ver los Admins" : "Click here to see all admins" %></p>
        <div class="admin-show-all">
          <%= render "admins/admin_all" %>
        </div>
      </div>
      <% end %>
    <div>
      <%= render "talks/talks.html.erb" %>
    </div>
  </div>
</div>
