<div id="online_users" class="currentuser-item-size-one">
  <div class="online-in">
    <%= link_to 'javascript:;', class: "btn btn-sm student-link-button", id: "who-is" do %>
      <% if @spanish %>
        <p>Â¿Quien esta en linea?</p>
      <% else %>
        <p>Who is online?</p>
      <% end %>
      <%= image_tag "users.png", class: "images-link-comunity" %> 
    <% end %>
    <div class="">
    <%= render "./tables/table.html.erb" %>
    </div>
    <div id="who-is-online" class="">
      <% all_users.each do |user| %>
        <% if user.user != current_user %>
          <p id="online<%= user.user.id %>">
            <%= link_to "#{user.user.name} #{user.user.lastname}", tables_path(user.user.id), :method => "create", class: "btn btn-sm online-link-button" %>
          </p>
        <% end %>
      <% end %>
    </div>
  </div>
</div>
